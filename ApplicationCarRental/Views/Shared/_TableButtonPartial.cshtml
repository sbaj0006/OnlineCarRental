@model ApplicationCarRental.Models.IndividualButtonPartial
@using ApplicationCarRental.Models

    <td style="width:400px;">
        <div class="btn-group" role="group">
            @Html.Partial("_IndividualButtonPartial",
                 new IndividualButtonPartial
                 {
                     Action = "Edit",
                     ButtonType = "btn-primary",
                     Glyph = "pencil",
                     Text = "Edit Button",
                     TypeId = Model.TypeId,
                     CarId = Model.CarId,
                     CustomerId = Model.CustomerId,
                     MembershiTypeId = Model.MembershiTypeId,
                     UserId = Model.UserId
                     
                 })
            @Html.Partial("_IndividualButtonPartial",
                 new IndividualButtonPartial
                 {
                     Action = "Details",
                     ButtonType = "btn-success",
                     Glyph = "list",
                     Text = "Details Button",
                     TypeId = Model.TypeId,
                     CarId = Model.CarId,
                     CustomerId = Model.CustomerId,
                     MembershiTypeId = Model.MembershiTypeId,
                     UserId = Model.UserId
                     
                 })
            @Html.Partial("_IndividualButtonPartial",
                 new IndividualButtonPartial
                 {
                     Action = "Delete",
                     ButtonType = "btn-danger",
                     Glyph = "trash",
                     Text = "Delete Button",
                     TypeId = Model.TypeId,
                     CarId = Model.CarId,
                     CustomerId = Model.CustomerId,
                     MembershiTypeId = Model.MembershiTypeId,
                     UserId = Model.UserId
                     
                 })
        </div>
    </td>
